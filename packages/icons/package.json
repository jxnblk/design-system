{
  "name": "pcln-icons",
  "version": "2.0.0-beta.3",
  "description": "Priceline React icons based on Material Design Icons",
  "main": "dist/Icon.js",
  "scripts": {
    "prepare": "npm run build && babel src -d lib",
    "clean": "find src/ -type f -not -name 'Icon.js' -delete && rm -rf svg/md lib",
    "build": "./build.js && pixo svg/md -d src && pixo svg/pcln -d src",
    "test": "jest"
  },
  "author": "Priceline",
  "license": "MIT",
  "dependencies": {
    "lodash.upperfirst": "^4.3.1",
    "styled-system": "^3.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "jest": "^23.5.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.uniqby": "^4.7.0",
    "material-design-icons": "^3.0.1",
    "pixo": "^1.1.1-0",
    "react": "^16.5.2",
    "react-test-renderer": "^16.5.2"
  },
  "peerDependencies": {
    "styled-components": ">=3.x"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "../../test-setup.js",
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ]
  },
  "pixo": {
    "template": "styledSystem"
  }
}
