{
  "name": "pcln-popover",
  "version": "3.3.2",
  "description": "React component for popover",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "build": "npm-run-all -p build:*",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel ./src --ignore '**/*.stories.js,**/*.spec.js' --out-dir ./dist/cjs",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel ./src --ignore '**/*.stories.js,**/*.spec.js' --out-dir ./dist/esm",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./src --ignore '**/*.stories.js,**/*.spec.js' --out-dir ./dist/es",
    "clean": "rimraf dist",
    "test": "jest"
  },
  "files": [
    "dist/"
  ],
  "author": "Priceline",
  "license": "MIT",
  "jest": {
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "setupFilesAfterEnv": [
      "../../test/test-setup.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "../../test"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.spec.js"
    ]
  },
  "dependencies": {
    "react-popper": "^1.3.3"
  },
  "peerDependencies": {
    "pcln-design-system": "^3.0.0",
    "styled-components": ">=3",
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@reach/component-component": "^0.7.0",
    "cross-env": "^6.0.3",
    "jest": "^26.0.1",
    "npm-run-all": "^4.1.5",
    "pcln-design-system": "^3.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-focus-lock": "^2.2.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.0",
    "styled-components": "^4.4.1",
    "styled-system": "^4.2.4"
  },
  "engines": {
    "node": ">=8.0.0 <9.0.0 || >=10.0.0",
    "npm": ">=5.10.0 || >=6.0.0"
  }
}
