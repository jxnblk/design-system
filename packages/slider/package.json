{
  "name": "pcln-slider",
  "version": "3.0.0",
  "description": "React component for range sliders",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "npm run build",
    "build": "npm-run-all -p build:*",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js ./src --out-dir ./dist/cjs",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js ./src --out-dir ./dist/esm",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./dist/es",
    "clean": "rimraf dist/",
    "test": "jest"
  },
  "files": [
    "dist/"
  ],
  "author": "Priceline",
  "license": "MIT",
  "jest": {
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.7.2",
    "rc-tooltip": "^3.7.0",
    "rc-util": "^4.0.4",
    "shallowequal": "^1.0.1",
    "styled-system": "^3.2.1",
    "warning": "^4.0.3"
  },
  "peerDependencies": {
    "pcln-design-system": ">=2.x",
    "styled-components": ">=3.x <5x",
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@reach/component-component": "^0.1.1",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "rc-tools": "8.x",
    "rc-trigger": "^2.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "styled-components": "^4.4.1"
  },
  "engines": {
    "node": ">=8.0.0 <9.0.0 || >=10.0.0",
    "npm": ">=5.10.0 || >=6.0.0"
  }
}
