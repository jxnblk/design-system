{
  "name": "pcln-slider",
  "version": "5.2.6",
  "description": "React component for range sliders",
  "license": "MIT",
  "author": "Priceline",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/pcln-slider.js",
      "require": "./dist/pcln-slider.cjs"
    }
  },
  "main": "./dist/pcln-slider.cjs",
  "module": "./dist/pcln-slider.js",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "vite build",
    "debug": "vite build --debug",
    "lint": "eslint .",
    "test": "vitest --ui --coverage",
    "test:ci": "vitest run --coverage",
    "test:coverage": "vitest run --coverage && vite preview --outDir ./coverage",
    "test:snapshots": "vitest --update",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@styled-system/prop-types": "^5.1.5",
    "classnames": "^2.3.2",
    "prop-types": "^15.8.1",
    "rc-slider": "^10.2.1",
    "rc-tooltip": "^6.0.1",
    "rc-util": "^5.35.0",
    "shallowequal": "^1.1.0",
    "styled-system": "^5.1.5",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@priceline/eslint-config": "workspace:*",
    "@reach/component-component": "^0.17.0",
    "@rushstack/eslint-config": "^3.3.2",
    "@rushstack/eslint-patch": "^1.3.2",
    "@testing-library/react": "^12.1.5",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^17.0.62",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.16",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "eslint": "^8.45.0",
    "jsdom": "^22.1.0",
    "pcln-design-system": "workspace:*",
    "pcln-icons": "workspace:*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "styled-components": "^5.3.11",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-bundle-visualizer": "^0.10.0",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-dts": "^3.3.1",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "pcln-design-system": "^5.0.0",
    "react": ">=16.10.0",
    "react-dom": ">=16.10.0",
    "styled-components": "^5.0.0"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6.0.0"
  }
}
