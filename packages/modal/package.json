{
  "name": "pcln-modal",
  "version": "5.4.13",
  "description": "React component for modal",
  "license": "MIT",
  "author": "Priceline",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel ./src --ignore '**/*.stories.js,**/*.spec.js' --out-dir ./dist/cjs",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel ./src --ignore '**/*.stories.js,**/*.spec.js' --out-dir ./dist/esm",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --verbose --coverage",
    "test:ci": "jest --silent --coverage --runInBand",
    "test:update": "jest --updateSnapshot",
    "test:snapshots": "jest --runInBand --updateSnapshot"
  },
  "dependencies": {
    "@reach/dialog": "^0.16.2",
    "@styled-system/theme-get": "^5.1.2",
    "react-transition-group": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.15",
    "@priceline/babel-preset": "workspace:*",
    "@priceline/eslint-config": "workspace:*",
    "@rushstack/eslint-patch": "^1.3.3",
    "@storybook/addon-actions": "^7.4.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^12.1.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-standard-reporter": "^2.0.0",
    "jest-styled-components": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "pcln-design-system": "workspace:*",
    "pcln-menu": "workspace:*",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.11",
    "styled-system": "^5.1.5"
  },
  "peerDependencies": {
    "pcln-design-system": "^5.22.0",
    "react": ">=16.10.0",
    "react-dom": ">=16.10.0",
    "styled-components": ">=4 <5 || >=5 <5.3.4 || >=5.3.7 <6",
    "styled-system": "^5.0.0"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6.0.0"
  }
}
